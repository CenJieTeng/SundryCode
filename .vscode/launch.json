    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch Log",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Log/Debug/Log.exe",
            "args": [
                "-v", "123",
                "-c",
                "--str", "hello world"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}